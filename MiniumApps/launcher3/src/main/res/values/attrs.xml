<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="siempo_slider">
        <attr name="wheel_start_angle" format="float" />
        <attr name="wheel_angle" format="float" />
        <attr name="wheel_border_thickness" format="dimension" />
        <attr name="wheel_border_color" format="color" />
        <attr name="wheel_active_color" format="string" />
        <attr name="wheel_inactive_color" format="string" />
        <attr name="wheel_title_color" format="string" />
        <attr name="wheel_subTitle_color" format="string" />
        <attr name="wheel_center_text_color" format="string" />
        <attr name="wheel_title_size" format="dimension" />
        <attr name="wheel_subTitle_size" format="dimension" />
        <attr name="wheel_center_text_size" format="dimension" />
        <attr name="thumb_size" format="dimension" />
        <attr name="thumb_image" format="reference" />
        <attr name="thumb_color" format="color" />
        <attr name="thumb_visible" format="boolean" />
        <attr name="thumb_init_position" format="integer" />
    </declare-styleable>

    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API level.
         ?android:attr/buttonBarStyle is new as of API 11 so this is
         necessary to support previous API levels. -->
    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="ClearableEditText">
        <attr name="clearIconDrawable" format="color|reference" />
        <attr name="clearIconDrawWhenFocused" format="boolean" />
    </declare-styleable>

    <declare-styleable name="icon">
        <attr name="icon_color" format="color" />
        <attr name="hint_color" format="color" />
        <attr name="image_alpha" format="color" />
        <attr name="pull_tab" format="color" />
        <attr name="chevron_junk" format="color" />
        <attr name="icon_color_reverse" format="color" />
        <attr name="icon_color_menu" format="color" />
        <attr name="txt_content_color" format="color" />
        <attr name="pop_up_color" format="color" />
        <attr name="txt_content_title" format="color" />
        <attr name="pane_color" format="color" />
        <attr name="indicator_active" format="color" />
        <attr name="indicator_inactive" format="color" />
        <attr name="toolbar_bg" format="color" />
        <attr name="bottom_doc" format="reference" />
        <attr name="bottom_doc_pos" format="reference" />
        <attr name="dialog_style" format="reference" />
        <attr name="top_doc" format="reference" />
        <attr name="top_date_color" format="color" />
        <attr name="toolbar_text_color" format="color" />
        <attr name="theme_base_color" format="color" />
        <attr name="button_bg" format="color" />
        <attr name="flag_header" format="color" />
        <attr name="card_image_trans" format="color" />
        <attr name="hint_search" format="color" />
        <attr name="card_image_trans_reverse" format="color" />
        <attr name="flag_foreground" format="color" />
        <attr name="divider_color" format="color" />
        <attr name="default_item_color" format="color" />
        <attr name="default_item_bg" format="color" />
        <attr name="divider_tools" format="color" />
        <attr name="rounded_edt_color" format="color" />
        <attr name="rounded_hint_color" format="color" />
        <attr name="junk_text" format="color" />
        <attr name="junk_top" format="color" />
        <attr name="status_bar_pane" format="color" />
        <attr name="navigation_bar_pane" format="color" />

        <attr name="highlighted_alpha" format="color" />
        <attr name="arrow_color" format="color" />
        <attr name="alphabet_color" format="color" />

    </declare-styleable>


</resources>
